{
  "name": "consent-management-provider",
  "version": "1.0.0",
  "description": "A minimal consent manager aiming for compliance to IAB's Transperancy and Consent Framework (TCF v2)",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node -r dotenv/config ./src/app.ts",
    "watch": "nodemon -e ts,js,template -w ./src -w ./templates -x npm run dev",
    "build": "rm -rf ./dist/* && tsc",
    "prod": "nodemon ./dist/app.js",
    "start": "npm run build && npm run prod"
  },
  "author": "Alexander Fut√°sz <alexander.futasz@fokus.fraunhofer.de>",
  "license": "UNLICENSED",
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.8",
    "express": "^4.17.1",
    "nodemon": "^2.0.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.8",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-plugin-import": "^2.22.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
